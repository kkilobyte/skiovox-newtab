{
  "manifest_version": 2,
  "version": "2.4.1",
  "name": "Skiovox New Tab",
  "description": "A New Tab page from the Skiovox exploit -- read more at skiovox.com",
  "icons": {
    "128": "icon.png"
  },
  "key": "",
  "commands": {
    "background": {
      "scripts": [
        "/background/commands.js",
        "/background/setup.js",
        "/background/webstore-fix.js"
      ]
    }
  },
  "chrome_url_overrides": {
    "newtab": "/new-tab/main.html"
  },
  "permissions": [
    "commands",
    "tabs",
    "sessions",
    "<all-urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "/content-scripts/window-open-fix.js"
      ],
      "all-frames": true,
      "run-at": "document_start"
    }
  ]
}